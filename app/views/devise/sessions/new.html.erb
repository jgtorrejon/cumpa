<div class="login-box">
    <div class="login-logo">
        <a href="#"><b>Cumpa</b></a>
    </div>
    <div class="login-box-body">
        <p class="login-box-msg">Nueva Sesion</p>
        <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
            <div class="form-group has-feedback">
                <%= f.email_field :email, autofocus: true, placeholder: "Correo electronico", class: "form-control" %>
            </div>
            <div class="form-group has-feedback">
                <%= f.password_field :password, autocomplete: "off", placeholder: "ContraseÃ±a", class: "form-control" %>
            </div>
            <div class="row">
                

                <div class="col-xs-12">
                    <%= f.submit "Log in", class: "btn btn-primary btn-block btn-flat" %>
                </div>
            </div>
        <% end %>
    </div>
</div>
